"set background=dark
hi clear
syntax reset
let g:colors_name = "generic"

"0 is black
"1 is red
"2 is green
"3 is yellow
"4 is blue
"5 is mag
"6 is cyn
"7 is white
"8 is bblack

"Standard syntax 
"Normal
hi Normal guifg=<%- fg %> guibg=<%- bg %>
"Boolean - red
hi Boolean ctermfg=01 guifg=<%- red %>
"Character - red
hi Character ctermfg=01 guifg=<%- red %>
"Comment - bblk
hi Comment ctermfg=08 guifg=<%- bblk %>
"Conditional - mag
hi Conditional ctermfg=05 guifg=<%- mag %>
"Constant - Blue
hi Constant ctermfg=04 guifg=<%- blu %>
"Define - Mag
hi Define ctermfg=05 guifg=<%- mag %>
"Float - Blue
hi Float ctermfg=04 guifg=<%- blu %>
"Function - Blue
hi Function ctermfg=01 guifg=<%- red %>
"Identifier - red
hi Identifier ctermfg=01 guifg=<%- red %>
"Include - blue
hi Include ctermfg=04 guifg=<%- blu %>
"Keyword - mag
hi Keyword ctermfg=05 guifg=<%- mag %>
"Label - yellow
hi Label ctermfg=03 guifg=<%- ylw %>
"Number - red
hi Number ctermfg=01 guifg=<%- red %>
"Operator - red
hi Operator ctermfg=01 guifg=<%- red %>
"PreProc - yellow
hi PreProc ctermfg=03 guifg=<%- ylw %>
"Repeat - yellow
hi Repeat ctermfg=03 guifg=<%- ylw %>
"Special - cyn
hi Special ctermfg=06 guifg=<%- cyn %>
"SpecialChar - cyn
hi SpecialChar ctermfg=06 guifg=<%- cyn %>
"Statement - red
hi Statement ctermfg=01 guifg=<%- red %>
"StorageClass - green
hi StorageClass ctermfg=03 guifg=<%- ylw %>
"String - green
hi String ctermfg=02 guifg=<%- grn %>
"Structure - mag
hi Structure ctermfg=05 guifg=<%- mag %>
"Tag - yellow
hi Tag ctermfg=03 guifg=<%- ylw %>
"Todo - yellow
hi Todo ctermbg=00 guibg=<%- blk %> ctermfg=05 guifg=<%- mag %>
"Type - yellow
hi Type ctermfg=03 guifg=<%- ylw %>
"Typedef - yellow
hi Typedef ctermfg=03 guifg=<%- ylw %>

" Other stuff
hi NERDTreeDirSlash ctermfg=05 guifg=<%- mag %>

hi MatchParen ctermbg=06 guibg=<%- cyn %> ctermfg=07 guifg=<%- wht %>

hi LineNr ctermbg=none guibg=<%- bg %> ctermfg=none guifg=<%- fg %>
hi CursorLineNr ctermbg=none guibg=<%- bg %> ctermfg=05 guifg=<%- mag %>

hi PMenu ctermbg=00 guibg=<%- blk %> ctermfg=05 guifg=<%- mag %>
hi PMenuSel ctermbg=05 guibg=<%- mag %> ctermfg=00 guifg=<%- blk %>

hi Directory ctermfg=05 guifg=<%- mag %>

hi CursorLine cterm=none gui=none guibg=<%- blk %>

hi Visual ctermbg=08 guibg=<%- bblk %> ctermfg=03 guifg=<%- ylw %>

hi Tabline ctermbg=00 guibg=<%- blk %> ctermfg=01 guifg=<%- red %> cterm=none gui=none
hi TablineFill ctermbg=00 guibg=<%- blk %> cterm=none gui=none
hi TablineSel ctermbg=08 guibg=<%- bblk %> ctermfg=03 guifg=<%- ylw %> cterm=none gui=none

hi StatusLine ctermbg=01 guibg=<%- red %> ctermfg=00 guifg=<%- blk %> cterm=none gui=none
hi StatusLineNC ctermbg=00 guibg=<%- blk %> ctermfg=01 guifg=<%- red %> cterm=none gui=none
hi User1 ctermbg=01 guibg=<%- red %> ctermfg=00 guifg=<%- blk %> 
hi User2 ctermbg=00 guibg=<%- blk %> ctermfg=01 guifg=<%- red %>

hi VertSplit ctermbg=00 guibg=<%- blk %> ctermfg=00 guifg=<%- blk %>
hi SignColumn ctermbg=00 guibg=<%- blk %> ctermfg=05 guifg=<%- mag %>

hi GitGutterAdd ctermbg=none guibg=<%- bg %> ctermfg=03 guifg=<%- ylw %> cterm=bold gui=bold
hi GitGutterChange ctermbg=none guibg=<%- bg %> ctermfg=03 guifg=<%- ylw %> cterm=bold gui=bold
hi GitGutterDelete ctermbg=none guibg=<%- bg %> ctermfg=03 guifg=<%- ylw %> cterm=bold gui=bold
hi GitGutterChangeDelete ctermbg=none guibg=<%- bg %> ctermfg=03 guifg=<%- ylw %> cterm=bold gui=bold 

hi ErrorMsg ctermbg=none guibg=<%- bg %> ctermfg=01 guifg=<%- red %> cterm=italic gui=italic

hi Title ctermfg=03 guifg=<%- ylw %>
hi NonText ctermfg=03 guifg=<%- blu %>

hi ALEErrorSign ctermfg=01 guifg=<%- red %> ctermbg=none guibg=<%- bg %>
hi ALEWarningSign ctermfg=03 guifg=<%- ylw %> ctermbg=none guibg=<%- bg %>
