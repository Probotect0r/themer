# vim: filetype=yaml
# dunst template
# Base16 <%- scheme %> Light, by <%- author %>

[urgency_low]
    background = "#<%- base["07"]["hex"] %>"
    foreground = "#<%- base["02"]["hex"] %>"

[urgency_normal]
    background = "#<%- base["07"]["hex"] %>"
    foreground = "#<%- base["0D"]["hex"] %>"

[urgency_critical]
    background = "#<%- base["07"]["hex"] %>"
    foreground = "#<%- base["08"]["hex"] %>"
